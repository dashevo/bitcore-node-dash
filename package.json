  "main": "./index.js",
  "repository": "git://github.com/dashevo/dashcore-node.git",
  "homepage": "https://github.com/dashevo/dashcore-node",
  "bugs": {
    "url": "https://github.com/dashevo/dashcore-node/issues"
  },
  "contributors": [
    {
      "name": "Christopher Jeffrey"
